{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
        {
            "files": [
              "**/*.test.js"
            ],
            "env": {
              "jest": true
            }
          }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "off"
        ],
        "consistent-return": [
            "off"
        ],
        "no-underscore-dangle": [
            "off"
        ],
        "no-unused-vars": [
            "warn"
        ]
    }
}
